<!DOCTYPE html>
@{
  Layout="Shared/_Layout.cshtml";
}

  <div class="container">
      <h1>Band Tracker</h1>
      <hr>
      <h2>@Model["band"].GetName()</h2><hr>
      <h3>Contact Info: @Model["band"].GetContact()<h3>
      <h4>Venues Played at:</h4>
      @foreach(var venue in Model["band"].GetVenues())
      {
        <li>@venue.GetName()</li>
      }

      <h4><a href="/add/band/venue">Add this band to a venue</a></h4>
      <hr>
      <h5><a href="/">Home</a></h5>
      <h5><a href="/add/band">Add.View Bands</a></h5>
      <h5><a href="/add/venue">Add Venue</a></h5>
    </div>
